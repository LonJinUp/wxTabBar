/* pages/tabbar2/index.wxss */
page{
  width: 100%;
  height: 100%;
  background: #275EFE;
}

.tabbar{
  width: 100%;
  height: 160rpx;
  position: fixed;
  bottom: 0;
  background: #fff;
  border-top-left-radius: 40rpx;
  border-top-right-radius: 40rpx;
  display: flex;
  justify-content: center;
  padding-top: 20rpx;

}


.tabbar .item{
  flex: 1;
  color: #fff;
  height: 80%;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  z-index: 10;


}

.tabbar .item.active .icon{
  color: #3561f5;
  transform: translateY(-55rpx);
  
}



.tabbar .item .icon{
  font-size: 50rpx!important;
  text-align: center;
  transition: all .8s;
  color: #bcc1de;
  
}

.tabbar .item .move{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fff;
}


.animation-target {
  box-shadow: 0 15rpx 30rpx rgb(0 0 0 / 10%);
  animation: animation 1401ms linear 1 both;
}

.tabbar .item .animation-target .icon{
  color: #275EFE;
}





